"""
Dokazete si predstavit prihodnejsi hru v Pythonu nez hada? ;o)

Pro praci se vstupem a vystupem hra pouziva modul Console
(http://effbot.org/zone/console-handbook.htm).

Pro vypis na konzoli je mozno pouzit 16 barev.
        
                0 = cerna (#000000)
                1 = modra (#0000a8)
                2 = tmave zelena (#00a800)
                3 = n/a (#00a8a8)
                4 = cervena (#a80000)
                5 = fuchsiova (#a800a8)
                6 = hneda (#a8a800)
                7 = svetle seda (#a8a8a8)
                8 = tmave seda (#545454)
                9 = n/a (#5454fc)
                10 = zelena (#54fc54)
                11 = azurova (#54fcfc)
                12 = n/a (#fc5454)
                13 = n/a (#fc54fc)
                14 = zluta (#fcfc54)
                15 = bila (#fcfcfc)

	Styl se da vypocitat nasledujici postupem:
                styl = barva_popredi + barva_pozadi * 16

        Defaultni barevny styl je svetle seda na cernem pozadi.

TODO:
        - pridat moznost hry vice hracu
        - na konci hry se pripojit na internet na nejaky php skript,
          ktery by uchovaval zebricek nejlepsich hracu

Martin Petr (farundel@gmail.com)
"""

import Console
import random
import time
import sys

# definice znaku a jejich barev pro vypis na obrazovku
BODY = ("*", 10) # telo hada
EMPTY = (" ", 0) # prazdny znak pouzity pri umazavani casti hada
FOOD = ("@", 14) # jidlo, ktere had sbira

PANEL_COLOR = 15 + 1 * 16 # barevny styl dolniho panelu

START_X = 26 # pocatecni x-ova souradnice hlavy hada
START_Y= 11
START_LENGTH = 5 # pocatecni delka hada
START_SPEED = 0.1 # pocatecni "rychlost" ve vterinach tak, jak ji akceptuje funkce sleep()
SPEED_STEP = 0.001 # o kolik budeme zvysovat rychlost v zavislosti na...
SCORE_STEP = 1 # hodnota poctu bodu pripadajicich na jedno zvyseni rychlosti

class IOHandler:
        """Trida objeku zpracovavajiciho vstup a vystup."""
        def __init__(self):
                self.c = Console.getconsole()
                self.c.title("Snake") # pojmenovani okna s konzoli
                self.c.cursor(0) # schovame blikajici kurzor

        def draw(self, x, y, what):
                """Vypise na zadane souradnice obsah promenne what."""
                self.c.text(x, y, what[0], what[1])
        
        def getKeyPress(self):
                """Precte vstup z klavesnice."""
                if self.c.peek():
                        return self.c.get()
                else:
                        return None
        
class Snake:
        """Trida objektu hada."""
        def __init__(self, x=START_X, y=START_Y, length=START_LENGTH, speed=START_SPEED):
                """Podle zadanych parametru vytvorime telo hada.

                Telo bude na zacatku vodorovne, bude mit delku length,
                hlava bude na souradnicich x, y a bude se pohybovat doprava."""
                self.body = [(x - i, y) for i in range(length)]
                self.score = 0
                self.direction = "r"
                self.speed = speed

                # vykreslime celeho hada
                for i in range(len(self.body)):
                        io.draw(self.body[i][0], self.body[i][1], BODY)
                
        def endGame(self):
                """Ukonci hru a vypise hlaseni."""
                io.draw(0, 24, ("Podarilo se ti nahrat " + str(self.score) + " bodu! Stiskni [ENTER] pro ukonceni...", PANEL_COLOR)) 
                while True:
                        if io.c.peek():
                                if io.c.get().keysym == "Return":
                                        break
                sys.exit(0)

        def setDirection(self, keyEvent):
                """Vybere smer podle obsahu udalosti keyEvent.
                
                Pokud zadna udalost z klavesnice nenastala, promenna
                direction se nemeni.

                Mozne hodnoty promenne direcion jsou:
                l - vlevo
                r - vpravo
                u - nahoru
                d - dolu."""
                if keyEvent:
                        if keyEvent.keysym == "Left":
                                self.direction = "l"
                        elif keyEvent.keysym == "Right":
                                self.direction = "r"
                        elif keyEvent.keysym == "Up":
                                self.direction = "u"
                        elif keyEvent.keysym == "Down":
                                self.direction = "d"

        def setNextCoordinates(self):
                """Podle daneho smeru pozmeni souradnice dalsi pozice hlavy."""
                # na zacatek seznamu body pripojime novy prvek (tj.
                # novou pozici hlavy v soucasne iteraci)
                if self.direction == "l":
                        self.body.insert(0, (self.body[0][0] - 1, self.body[0][1]))
                elif self.direction == "r":
                        self.body.insert(0, (self.body[0][0] + 1, self.body[0][1]))
                elif self.direction == "u":
                        self.body.insert(0, (self.body[0][0], self.body[0][1] - 1))
                elif self.direction == "d":
                        self.body.insert(0, (self.body[0][0], self.body[0][1] + 1))
                
                # pokud jsme vyjeli mimo obrazovku, ukoncime hru
                if self.body[0][0] < 0 or \
                   self.body[0][1] < 0 or \
                   self.body[0][0] > 79 or \
                   self.body[0][1] > 23:
                        self.endGame()

                # pokud jsme narazili sami do sebe, tak hru taky ukoncime
                if self.body[0] in self.body[1:]:
                        self.endGame()

        def nextMove(self):
                """Vykresli novou pozici hada."""
                # posuneme hlavu hada na obrazovce na novou pozici
                io.draw(self.body[0][0], self.body[0][1], BODY)
                
                # pokud jsme nenarazili na potravu "umazeme" jeho ocas a vratime False
                if self.body[0] != food.coords:
                        io.draw(self.body[len(self.body) - 1][0],
                                self.body[len(self.body) - 1][1], EMPTY)
                        # a nakonec odstranime posledni prvek seznamu
                        self.body.pop()
                        return False
                else:
                        self.score += 1
			# rychlost zvysujeme pouze pri urcitem poctu dosazenych bodu
                        if self.score % SCORE_STEP == 0:
                                self.speed -= SPEED_STEP
                        return True

class Food:
        """Trida charakterizujici jidlo."""
        def __init__(self):
                self.createFood()
                self.drawFood()

        def createFood(self):
                """Vytvori jidlo pro hada na nahodne pozici.
                
                Dokud neni vytvarene jidlo mimo souradnice nektere z
                casti hadova tela, cyklus pokracuje."""
                while True:
                        x = random.randint(0, 79)
                        y = random.randint(0, 23)
                        if not (x, y) in snake.body:
                                break
                self.coords = (x, y)

        def drawFood(self):
                """Vykresli jidlo na pozici."""
                io.draw(self.coords[0], self.coords[1], FOOD)


class BottomPanel:
	"""Trida objektu dolniho panelu.

	Na panelu se zobrazuje dosazene skore."""
	def __init__(self):
		self.redraw(snake.score)

	def redraw(self, score):
		"""Vypise udaje o hadovi."""
                string = "Score: " + str(score) 
                io.draw(0, 24, (string + " " * (80 - len(string)), PANEL_COLOR))

io = IOHandler()
snake = Snake()
food = Food()
panel = BottomPanel()

while True:       
        snake.setDirection(io.getKeyPress())
        snake.setNextCoordinates()
        iAteNow = snake.nextMove()
        if iAteNow: # pokud jsme prave snedli nejakou dobrotu, uvarime novou
                food.createFood()
                food.drawFood()
                iAteNow = False
	panel.redraw(snake.score)
        time.sleep(snake.speed)

