import serial
import time
import threading

def scan_COM_ports():
  """scan for available ports. return a list of tuples (num, name)"""
  available =3D []
  for i in range(256):
      try:
          s =3D serial.Serial(i)
          available.append( (i, s.portstr))
          s.close()   #explicit close 'cause of delayed GC in java
      except serial.SerialException:
          pass
  print "nalezene com porty:"
  for n,s in available:
    print "(%d) %s" % (n,s)


class Timer(threading.Thread):
  def __init__(self,sleep,port,action):
    self.sleep=3Dsleep
    self.port=3Dport
    self.action=3Daction
    threading.Thread.__init__(self)

  def run(self):
    while 1:
      time.sleep(self.sleep)
      buff=3Dself.port.inWaiting()
      if buff>0:
        data=3Dself.port.read(buff)
        self.action(data)

def zpracuj_packet(data):
  print "zpracovavam packet:",data

def main():
  scan_COM_ports()
  a=3Draw_input("vyber COM port: ")
  ser=3Dserial.Serial(a,baudrate=3D1200)

  my_timer=3DTimer(0.01,ser,zpracuj_packet)
  my_timer.start()



