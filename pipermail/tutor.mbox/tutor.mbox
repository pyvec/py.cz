from math import sin, cos, pi

# pocet krokov
steps =3D 100

# priemer
d =3D 50

# stred
x =3D 320
y =3D 240

# polomer
r =3D d/2

for i in range(steps):
     # dlzka obluku na jednotkovej kruznici
     arc =3D i * (2*pi/steps)

     # prevod
     print (x+(cos(arc)*r), y+(sin(arc)*r))

btw je to skor stredoskolska matika ako python

