#!/usr/bin/env python
# -*- coding: UTF-8 -*-
import sys,os
import pygtk 
pygtk.require("2.0")  
import gtk 
import gtk.glade

APPNAME="mpPanorama"
APPVERSION="0.1"



class widgetWrapper:
  """ Vytvoreni 'inteligentnejsiho' widgetu """
  
  def __init__(self, file_name, widget_name, handlers_class):
    """ Konstruktor """
    self.widgets = gtk.glade.XML(file_name, widget_name)
    self.widgets.signal_autoconnect(handlers_class.__dict__)
    print handlers_class.__dict__ # Nechapu, proc tady neni napriklad i "on_window_destroy"
  
  def __getitem__(self, key):
    """ Diky teto fci lze volat jmeno_instance['jmeno_widgetu'] """
    return self.widgets.get_widget(key)



class windowHandlers:
  """ Obsluha udalosti widgetu
  window - globalni promenna - instance tridy widgetsWrapper """
  
  def __init__(self, file_name):
    self.glade_file = file_name
    self.window = widgetWrapper(self.glade_file, 'window', self)
  
  def on_window_destroy(self):
    gtk.main_quit()



if __name__ == "__main__":
  window = windowHandlers("mpGui.glade")
  gtk.main()





