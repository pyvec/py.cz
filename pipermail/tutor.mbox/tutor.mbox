import sys
import os
import re


bin = "/sbin/ifconfig"
iface = str(raw_input("\nZadejte interface: "))

shell = bin+" "+iface
opensh = os.popen(shell)
for lineout in opensh.readlines():
	mode = re.match('\w+\s+Link\s+encap:(?P<MODE>\w+)\s+HWaddr', lineout)
	print mode.group('MODE')


