import sys,traceback

def get_exception():
  exc=3Dsys.exc_info()
  sys.exc_clear()
  ex_type=3Dexc[0]
  ex_fault=3Dexc[1]
  ex_trace_all=3Dtraceback.extract_tb(exc[2])
  ex_str=3D'Traceback (most recent call last):\n'
  for ex_trace in ex_trace_all:
    ex_str+=3D'  File "'+ex_trace[0]+'", line '+str(ex_trace[1])+', in
'+ex_trace[2]+'\n'
    ex_str+=3D'    '+ex_trace[3]+'\n'
  ex_str+=3Dstr(ex_type)+': '+str(ex_fault)
  return ex_str

