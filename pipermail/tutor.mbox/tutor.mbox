from ftplib import FTP
import string

def smaz(domena,uzivatel,heslo,slozka):
    soubory=3D[]
    ftp =3D FTP(domena)
    ftp.login(user=3Duzivatel,passwd=3Dheslo)
    polozky,odpoved=3Dftp.retrlines("LIST "+slozka)

    for polozku in polozky:
        soubor=3Dstring.split(polozku)
        if not len(soubor) < 1:
            soubory.append(soubor[-1])
    odpoved=3Draw_input("Chcete pokracovat? (a/n): ")
    if odpoved =3D=3D "a" or odpoved =3D=3D "A":
        for soubor in soubory:
            if soubor =3D=3D".." or soubor=3D=3D".":
                pass
            else:
                print "Mazu: %s" % soubor
                ftp.delete(slozka+"\\"+soubor)
        ftp.quit()
    else:
        print "Akce zrusena"
        ftp.quit()

